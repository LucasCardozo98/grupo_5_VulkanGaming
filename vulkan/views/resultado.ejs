<%- include("partials/head")%> 
<%- include("partials/header")%> 


<main class="container mt-2 mb-2">
  <section class="contenedora">
    <div class="container">
      <div class="row">
          <div class="slider owl-carousel">
        <% products.forEach(element=> { %>
            <div class="card col-sm-12 col-md-5 col-lg-3">
              <div class="img">
                <img src="/images/<%= element.image %> " alt="">
              </div>
              <div class="content">
                <div class="title" id="nombre" idProducto="<%= element.id %>" >
                  <%= element.name %>
                </div>
                <div class="price">
                  <%= element.price %>
                </div>
                <div class="btn">
                  <button type="submit" onclick="agregar()"><i class="fas fa-cart-plus"></i> Agregar al
                    carrito</button>
                </div>
              </div>
            </div>
          <% }) %>
          </div>
      </div>
    </div>
  </section>
</main>

<%- include("partials/footer")%> 
<script src="/javascripts/carritoDos.js" ></script>

</body>